{
    "title": "Binds fn + F1-F5 to media keys",
    "rules": [
        {
            "description": "Binds fn + F1-F5 to media keys",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f1",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "play_or_pause"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 1241,
                                    "product_id": 17733
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f2",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "volume_decrement"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 1241,
                                    "product_id": 17733
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f3",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "volume_increment"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 1241,
                                    "product_id": 17733
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f4",
                        "modifiers": {
                            "mandatory": [
                                "fn"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "rewind"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 1241,
                                    "product_id": 17733
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
