                        }
                    ],
                    "conditions": [
                        {
                            "type": "device_if",
                            "identifiers": [
                                {
                                    "vendor_id": 1241,
                                    "product_id": 17733
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
