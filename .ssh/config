Include /Users/simon/.colima/ssh_config

Include /Users/simon/.config/colima/ssh_config

Include config_local

Host *
  SetEnv TERM="xterm-256color"
Host astoria
  User simon
  HostName astoria
  SetEnv LC_ALL="C"
Host pi3
  User pi
  HostName pi3
  IdentityFile ~/.ssh/id_ed25519

Host *
  ForwardAgent no
  AddKeysToAgent no
  Compression no
  ServerAliveInterval 0
  ServerAliveCountMax 3
  HashKnownHosts no
  UserKnownHostsFile ~/.ssh/known_hosts
  ControlMaster no
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist no

  EnableEscapeCommandline=yes
