Include config_local

Host astoria
  User simon
  HostName astoria.local
  SetEnv LC_ALL="C"

Host astoria.ts
  User simon
  HostName astoria.tortoise-bearded.ts.net
  SetEnv LC_ALL="C"

Host pi3
  User pi
  HostName pi3
  IdentityFile ~/.ssh/id_ed25519

Host *
  ForwardAgent no
  AddKeysToAgent no
  Compression no
  ServerAliveInterval 0
  ServerAliveCountMax 3
  HashKnownHosts no
  UserKnownHostsFile ~/.ssh/known_hosts
  ControlMaster no
  ControlPath ~/.ssh/master-%r@%n:%p
  ControlPersist no

  EnableEscapeCommandline=yes
